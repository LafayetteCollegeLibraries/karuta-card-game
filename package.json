{
  "name": "preact-card-game",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=\"production\" webpack -p",
    "dev": "webpack-dev-server",
    "page": "npm run page:start && npm run page:build && npm run page:cleanup",
    "page:build": "npm run build:prod",
    "page:cleanup": "npm run page:move && npm run rm:all",
    "page:start": "git checkout -B gh-pages",
    "page:move": "mv public/* .",
    "rm:all": "npm run rm:config && npm run rm:dirs",
    "rm:config": "rimraf .{babelrc,gitignore} webpack.*.js readme.markdown game-config.json",
    "rm:dirs": "rimraf node_modules/ public/ src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
