  "license": "GPL-3.0",
  "dependencies": {
    "lodash.shuffle": "^4.2.0",
    "preact": "^8.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
